  <div class="row">
    <div class="span4">

      <div class="control-group">
        <%= f.label :logradouro, nil, :class => 'control-label' %>
        <div class="controls">
          <%= f.text_field :logradouro, "data-validate" => "validate(required) on(blur change)" %>
        </div>
      </div>

      <div class="control-group">
        <%= f.label :complemento, nil, :class => 'control-label' %>
        <div class="controls">
          <%= f.text_field :complemento, "data-validate" => "validate(required) on(blur change)" %>
        </div>
      </div>

    </div><!--end span4-->

    <div class="span4">

      <div class="control-group">
        <%= f.label :bairro, nil, :class => 'control-label' %>
        <div class="controls">
          <%= f.text_field :bairro, "data-validate" => "validate(required) on(blur change)" %>
        </div>
      </div>

      <div class="control-group">
        <%= f.label :cep, nil, :class => 'control-label' %>
        <div class="controls">
          <%= f.text_field :cep, :class => '_cep', "data-validate" => "validate(required) on(blur change)" %>
        </div>
      </div>

    </div><!--end span4-->

    <div class="span4">

      <div class="control-group">
        <%= f.label :municipio_id, nil, :class => 'control-label' %>
        <div class="controls">
          <%= f.text_field :municipio_id, :class => '_municipio', 'data-pre' => f.object.token_municipio, "data-validate" => "validate(token_input) on(blur change)" %>
