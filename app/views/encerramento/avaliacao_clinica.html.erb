<h1>Avaliação clínica de encerramento</h1>

<%= render 'partials/escolher_voluntario', :usuario => true %>

<% unless session[:voluntario].blank? %>
  <ul class='unstyled horizontal-list'>
    <li><%= link_to 'Dados demográficos', encerramento_dados_demograficos_path %></li>
    <li><%= link_to 'Sinais vitais', encerramento_sinais_vitais_path %></li>
