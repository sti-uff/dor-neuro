<h1>Verificar teste de gravidez</h1>

<%= render 'partials/escolher_voluntario', :usuario => false %>

<% if @gravidez %>
  <div>
    <%= image_tag @gravidez.imagem.url %>
  </div>
<% else %>
  <p>O teste de gravidez desta voluntária não foi registrado.</p>
<% end %>

<%= link_to 'Voltar', show_exames_path, :class => 'btn' %>