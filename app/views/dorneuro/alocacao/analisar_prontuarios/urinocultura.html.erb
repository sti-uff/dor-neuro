<h1>Verificar urinocultura</h1>

<%= render 'partials/escolher_voluntario', :usuario => false %>

<% if @urinocultura %>
  <div>
    <%= image_tag @urinocultura.imagem.url %>
  </div>
<% else %>
  <p>A urinocultura deste voluntário não foi registrada.</p>
<% end %>

<%= link_to 'Voltar', show_exames_path, :class => 'btn' %>