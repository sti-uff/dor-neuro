<h1>Verificar hemograma completo</h1>

<%= render 'partials/escolher_voluntario', :usuario => false %>

<% if @hemograma %>
  <div>
    <%= image_tag @hemograma.imagem.url %>
  </div>
<% else %>
  <p>O hemograma deste voluntário não foi registrado.</p>
<% end %>

<%= link_to 'Voltar', show_exames_path, :class => 'btn' %>